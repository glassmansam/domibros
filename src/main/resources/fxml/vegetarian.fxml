<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Font?>
<fx:root
        style="-fx-border-style: solid solid solid solid; -fx-border-width: 1px; -fx-border-radius: 5px; -fx-border-color: #27ae60;"
        text="Vegetarian" textFill="#229451" type="Label" xmlns="http://javafx.com/javafx/16"
        xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="3.0" left="5.0" right="5.0" top="3.0"/>
   </padding>
   <graphic>
      <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/icons/leaf.png"/>
         </image>
      </ImageView>
   </graphic>
   <font>
      <Font name="System Italic" size="12.0"/>
   </font>
   <cursor>
      <Cursor fx:constant="DEFAULT"/>
   </cursor>
</fx:root>
